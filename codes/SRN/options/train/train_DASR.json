{
  "name": "0603_DASR_SRN_face_a",
  "use_tb_logger": true,
  //"model": "DASR_FS_ESRGAN_patchGAN",
  "model": "DASR",
  "scale": 4,
  "gpu_ids": [
    0
  ],
  "chop": false, // during testing, crop input images.
  "val_lpips": true, // during testing, show LPIPS.
  "multiweights": true, // use domain-distance maps
  "datasets": {
    "train": {
      "name": "DIV2K",
      "mode": "LRHR_wavelet_unpair_fake_weights_EQ",
      "dataroot_HR": "D:/Dataset/face_dataset_comp/train_HR", // target domain
      "dataroot_fake_LR": "C:/Users/madin/Documents/PhD/Semester 3/AI 604/Project/AI604_project/DSN_results/0603_DSN_LRs_realsr/imgs_from_target", // generated LRs
      "dataroot_real_LR": "D:/Dataset/face_dataset_comp/train_source", // source domain
      "dataroot_fake_weights": "C:/Users/madin/Documents/PhD/Semester 3/AI 604/Project/AI604_project/DSN_results/0603_DSN_LRs_realsr/ddm_target", // domain-distance map
      "subset_file": null,
      "use_shuffle": true,
      "n_workers": 8,
      "batch_size": 4, // batch size will be 12(two times of original)since [reallr*6, fakelr*6]
      "HR_size": 128,
      "use_flip": true,
      "use_rot": true
    },
    "val": {
      "name": "validation",
      "mode": "LRHR",
      "dataroot_HR": "D:/Dataset/face_dataset_comp/valid_HR",
      "dataroot_LR": "D:/Dataset/face_dataset_comp/valid_LR"
    }
  },
  "path": {
    "root": "../../SRN_experiments",
    "resume_state": null
    //"pretrain_model_G": "/media/4T/Dizzy/github/DASR/premodel/12500_G.pth",
    //"pretrain_model_D_target": "/media/4T/Dizzy/github/DASR/premodel/12500_D_target.pth",
    //"pretrain_model_D_source": null
  },
  "network_G": {
    "which_model_G": "RRDB_net",
    "norm_type": null,
    "mode": "CNA",
    "nf": 64,
    "nb": 23,
    "in_nc": 3,
    "out_nc": 3,
    "gc": 32,
    "group": 1,
    "reduction": 16,
    "nbht": 4
  },
  "network_D": {
    "which_model_D": "discriminator_patch",
    "which_model_pairD": "discriminator_vgg_128",
    "norm_type": "Batch",
    "act_type": "leakyrelu",
    "mode": "CNA",
    "nf": 64,
    "in_nc": 3,
    "n_layers": 2
  },
  "train": {
    "lr_G": 1e-5,
    "weight_decay_G": 0,
    "beta1_G": 0.9,
    "lr_D": 1e-5,
    "weight_decay_D": 0,
    "beta1_D": 0.9,
    "lr_scheme": "MultiStepLR",
    "lr_steps": [
      10000,
      80000,
      100000,
      150000
    ],
    "lr_gamma": 0.5,
    "fs": "gau",
    "fs_kernel_size": 9,
    "norm": true,
    "sup_LL": true,
    "pixel_criterion": "l1",
    "pixel_weight": 1,
    "pixel_LL_weight": 1,
    "feature_criterion": "LPIPS",
    "feature_weight": 1,
    "gan_type": "vanilla",
    "ragan": false,
    "gan_H_target": 0.01,
    "gan_H_source": 0,
    "G_update_inter": 1,
    "D_update_inter": 1,
    "manual_seed": 0,
    "niter": 350000.0,
    "val_freq": 2500.0,
    "save_tsamples": 200.0
  },
  "logger": {
    "print_freq": 200,
    "save_checkpoint_freq": 2500.0
  }
}